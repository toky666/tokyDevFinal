---
import Subtitle from "@src/components/atoms/Subtitle/Subtitle.astro";
const discordData  = {
  title: "Join our Discord community",
  content: "Connect with web developers, share resources, and get help on your frontend projects.",
  link: "https://discord.gg/xGHuzuYQ",
  image: "../img/icons/DiscordLogo.png",
};

---

<div class="card-social__container">
<Subtitle content="Join our Discord community" />
  <p class="card-social__content">
    Connect with IT and auditing professionals, share tools and methodologies, and get support for your technology projects and control processes.<iconify-icon icon="skill-icons:discord" width="15" height="15"></iconify-icon>
  </p>
  <a class="card-social__link" href="https://discord.gg/xGHuzuYQ" target="_blank" rel="noreferrer noopener nofollow">
    Join our community <iconify-icon
      icon="quill:link-out"
      width="15"
      height="15"
      style="color: aqua"></iconify-icon></a
  >
  <img
    class="card-social__image"
    src="../img/icons/DiscordLogo.png"
    alt="Discord logo"
  />
</div>
